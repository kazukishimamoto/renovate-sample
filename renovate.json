{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:recommended"],
  "timezone": "Asia/Tokyo",
  "packageRules": [
    {
      "matchManagers": ["npm"],
      "matchPackageNames": ["*"],
      "enabled": false
    },
    {
      "matchManagers": ["npm"],
      "matchPackageNames": ["prettier", "vue-router"],
      "enabled": true
    }
  ]
}
